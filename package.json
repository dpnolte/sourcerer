{
  "name": "sourcerer-android",
  "version": "0.0.3",
  "description": "data objects and typescript definitions for the generated android json adapters",
  "main": "./js-dist/index.js",
  "files": [
    "js-dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpnolte/sourcerer.git"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@types/jest": "^23.3.9",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "typescript": "latest"
  },
  "keywords": [
    "android",
    "typescript",
    "javascript",
    "json",
    "adapter"
  ],
  "author": "Derk Nolte",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dpnolte/sourcerer/issues"
  },
  "homepage": "https://github.com/dpnolte/sourcerer#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "__TEST__": true
    },
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
